<Window x:Class="Pacu_Man.LoginGame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        xmlns:local="clr-namespace:Pacu_Man"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Left="480" Top="250"
        Title="PACU-MAN" Height="545" Width="960">
    <Window.Background>
        <ImageBrush ImageSource="images/login.png"></ImageBrush>
    </Window.Background>
    <Grid>
        <Canvas Name="MyCanvas" KeyDown="CanvasKeyDown" Focusable="True" Margin="0,0,-0.4,0.2" >
            <Image gif:ImageBehavior.AnimatedSource="Images/pacman.gif" Canvas.Left="382" Canvas.Top="115" Height="42" Width="75"  />

            <Image Source="images/clicklogin.png" x:Name="lab1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="34" Width="338" Canvas.Left="310" Canvas.Top="424" />
        </Canvas>
    </Grid>
</Window>
